{
  "name": "@mcf/server-boilerplate-middleware",
  "version": "0.8.2",
  "description": "Boilerplate middleware for Node projects in MyCareersFuture.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx babel --presets=env src --out-dir dist --ignore '*.test.js' ",
    "clean": "rm -rf ./dist/*",
    "lint": "npx eslint -c ../../.eslintrc.json .",
    "test-watch": "npx mocha --watch --require babel-register \"src/*.test.js\" \"src/**/*.test.js\" ",
    "test": "npx mocha --require babel-register \"dist/*.test.js\" \"dist/**/*.test.js\"",
    "pretest": "cd ./src && find ./ -name '*.test.js' | cpio -pdm ../dist",
    "posttest": "find ./dist -name '*.test.js' | xargs -I@ rm -rf @",
    "prestart": "npm run build",
    "start": "nodemon ./test/server.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "babel": {
    "presets": [
      "env"
    ],
    "ignore": [
      "node_modules"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GovTechSG/mcf-boilerplate-js.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mcf/tracer": "^0.0.6",
    "body-parser": "^1.18.3",
    "case": "^1.5.5",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-prom-bundle": "^3.3.0",
    "helmet": "^3.12.1",
    "morgan": "^1.9.0",
    "prom-client": "^11.0.0",
    "zipkin": "^0.14.0",
    "zipkin-instrumentation-express": "^0.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "nodemon": "^1.18.3",
    "npx": "^10.2.0",
    "supertest": "^3.0.0"
  }
}
