{
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules",
    "tslint-plugin-prettier",
    "tslint-config-prettier",
    "tslint-no-unused-expression-chai"
  ],
  "rules": {
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,
    "no-unused-variable": [true, {"ignore-pattern": "^_"}],
    "prettier": true
  }
}
