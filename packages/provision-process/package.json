    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "express": "^4.16.3",
    "nodemon": "^1.18.3",
    "npx": "^10.2.0",
    "q": "^1.5.1",
    "supertest": "^3.0.0"
  }
