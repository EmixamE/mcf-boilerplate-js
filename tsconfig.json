{
  "compilerOptions": {
    "declaration": true,
    "module": "commonjs",
    "lib": [
      "es5",
      "es2015"
    ],
    "alwaysStrict": true,
    "sourceMap": true,
    "listEmittedFiles": true,
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target": "es5"
  }
}